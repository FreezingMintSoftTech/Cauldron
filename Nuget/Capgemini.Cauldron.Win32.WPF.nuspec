      <dependency id="Capgemini.Cauldron.Cryptography" version="3.2.2" />
      <dependency id="Capgemini.Cauldron.Localization" version="3.2.2" />
      <dependency id="Capgemini.Cauldron.Win32.MonitorInfo" version="3.2.2" />
      <dependency id="Capgemini.Cauldron.Win32.UserInformation" version="3.2.2" />
      <dependency id="Capgemini.Cauldron.Win32.WPF.ParameterPassing" version="3.2.2" />
    </dependencies>
    <contentFiles>
      <files include="**/Interceptors/*.*" buildAction="None" />
